.modal#kairanyouken-new-modal{tabindex:-1, role: :dialog, 'aria-labelledby'=>"myModalLabel", 'aria-hidden'=>"true"}
  .modal-dialog.modal-sm
    .modal-content
      .modal-header
        %button.close{type: :button, 'data-dismiss'=>"modal", 'aria-hidden'=>"true"} &times;
        %h4.modal-title#myModalLabel New
      .modal-body
        = simple_form_for (Kairanyokenmst.new), url: create_kairanyoken_kairanyokenmsts_path, :remote => true, html: {role: :form, 'data-model' => 'kairanyokenmst'} do |f|
          = f.input :名称 do
            = f.input_field :名称, class: 'form-control'
            .span.help-block
          = f.input :備考 do
            = f.input_field :備考, class: 'form-control'
            .span.help-block
          = f.input :優先さ,label: "優先さ"
          = f.button :submit, class: 'btn btn-primary'