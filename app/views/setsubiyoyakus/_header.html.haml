.row-fruid
  .form-inline.row
    .col-xs-2
      = label_tag :機能
    .col-xs-4
      = select(:head, :setsubicode, Setsubi.all.order(:設備コード).collect {|p| [ p.設備名, p.設備コード ] } , {selected: @setsubi_param, include_blank: true }, class: 'form-control')
    .col-xs-3
      = button_tag '検索', class:'btn btn-primary'
